@article{alloccaValidationSomnivoreMachine2019,
  title = {Validation of `{{Somnivore}}', a {{Machine Learning Algorithm}} for {{Automated Scoring}} and {{Analysis}} of {{Polysomnography Data}}},
  author = {Allocca, Giancarlo and Ma, Sherie and Martelli, Davide and Cerri, Matteo and Del Vecchio, Flavia and Bastianini, Stefano and Zoccoli, Giovanna and Amici, Roberto and Morairty, Stephen R. and Aulsebrook, Anne E. and Blackburn, Shaun and Lesku, John A. and Rattenborg, Niels C. and Vyssotski, Alexei L. and Wams, Emma and Porcheret, Kate and Wulff, Katharina and Foster, Russell and Chan, Julia K. M. and Nicholas, Christian L. and Freestone, Dean R. and Johnston, Leigh A. and Gundlach, Andrew L.},
  year = {2019},

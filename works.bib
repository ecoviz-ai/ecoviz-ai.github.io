@article{hassrickConditionMassImpact2010,
  title = {Condition and Mass Impact Oxygen Stores and Dive Duration in Adult Female Northern Elephant Seals},
  author = {Hassrick, J. L. and Crocker, D. E. and Teutschel, N. M. and McDonald, B. I. and Robinson, P. W. and Simmons, S. E. and Costa, D. P.},
  year = {2010},
  month = feb,
  journal = {Journal of Experimental Biology},
  volume = {213},

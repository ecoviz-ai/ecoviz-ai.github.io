@article{williamsElectroencephalogramHealthyHorses2016,
  title = {Electroencephalogram of {{Healthy Horses During Inhaled Anesthesia}}},
  author = {Williams, D. C. and Aleman, M. R. and Brosnan, R. J. and Fletcher, D. J. and Holliday, T. A. and Tharp, B. and Kass, P. H. and Steffey, E. P. and Lecouteur, R. A.},
  year = {2016},
  journal = {Journal of Veterinary Internal Medicine},
  volume = {30},

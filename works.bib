@article{lyaminMuscleJerksBehavioral2002,
  title = {Muscle Jerks during Behavioral Sleep in a Beluga Whale ({{Delphinapterus}} Leucas {{L}}.)},
  author = {Lyamin, O and Shpak, O and Nazarenko, E and Mukhametov, L},
  year = {2002},

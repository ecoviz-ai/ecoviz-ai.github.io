@article{fauvilleUnderwaterVirtualReality2024,
  title = {Underwater Virtual Reality for Marine Education and Ocean Literacy: Technological and Psychological Potentials},
  shorttitle = {Underwater Virtual Reality for Marine Education and Ocean Literacy},
  author = {Fauville, G{\'e}raldine and Vo{\c s}ki, Ana{\"i}s and Mado, Marijn and Bailenson, Jeremy N. and {Lantz-Andersson}, Annika},
  year = {2024},
  journal = {Environmental Education Research},
  volume = {0},
  number = {0},
  pages = {1--25},
  publisher = {Routledge},
  issn = {1350-4622},
  doi = {10.1080/13504622.2024.2326446},
  urldate = {2024-06-04},
  abstract = {Given the crucial role the ocean plays in human and planetary health, ocean literacy (OL)---understanding the ocean's influence on us and our influence on the ocean---has been gaining momentum as a significant component and determinant of sustainable human-ocean interactions. Underwater virtual reality (UVR) enables participants to take a virtual dive while being simultaneously immersed in water. Through this unique double immersion, UVR is offering new avenues of psychological access and pedagogical opportunities toward the ocean; this study is thus the first to explore UVR's potential for OL and marine environmental education. Following two UVR activities in a pool, we conducted semi-structured interviews and survey questionnaires with 19 marine scientists and education experts. Our findings indicate UVR has important technological and psychological potentials, including (1) high levels of presence, (2) infrequency of motion sickness, and (3) the ability to trigger powerful emotions and affective states such as awe, empathy, and flow. In this way, UVR can create a realistic experience of the ocean that improves OL and reduces psychological distance to marine environmental issues. We also advance a new seven-dimensional OL framework, following our findings that UVR has unique potentials for improving `emoceans', attitude, awareness, and ocean connectedness.},
  keywords = {affect,awe,emotions,marine education,notion,ocean literacy,psychological distance,SDG 14: Life below water,Underwater virtual reality,virtual dive,virtual reality},
  file = {/Users/jessiekb/Zotero/storage/SWKNK5U8/Fauville et al. - 2024 - Underwater virtual reality for marine education an.pdf}
}

@article{fedakCirculatoryResponsesSeals1988,
  title = {Circulatory Responses of Seals to Periodic Breathing: Heart Rate and Breathing during Exercise and Diving in the Laboratory and Open Sea},
  shorttitle = {Circulatory Responses of Seals to Periodic Breathing},
  author = {Fedak, M. A. and Pullen, M. R. and Kanwisher, J.},

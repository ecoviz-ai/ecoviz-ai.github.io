@article{lyaminSleepNorthernFur2017,
  title = {Sleep in the Northern Fur Seal},
  author = {Lyamin, O. I. and Mukhametov, Lev M and Siegel, Jerome M},
  year = {2017},
  month = jun,
  journal = {Current Opinion in Neurobiology},
  series = {Neurobiology of {{Sleep}}},
  volume = {44},
  pages = {144--151},
  issn = {0959-4388},
  doi = {10.1016/j.conb.2017.04.009},
  urldate = {2021-01-02},
  abstract = {The pattern of sleep in the fur seal, a semiaquatic pinniped, has several striking behavioral and physiological adaptations that allow this species to inhabit both the land and water environment. These features include unihemispheric slow wave sleep (USWS, also being unihemispheric waking), the ability to maintain movement for stabilization of the sleep posture and to briefly open one eye while having a sleep electroencephalogram (EEG) in one hemisphere. In vivo microdialysis studies suggest that acetylcholine release is required for cortical activation during USWS, and that monoamines are not required for USWS. The need to breathe, to maintain efficient thermoregulation, and to avoid predation have shaped the sleep patterns in semiaquatic fur seals as in fully aquatic cetaceans.},
  langid = {english},
  file = {/Users/jessiekb/Zotero/storage/24P7FUM9/Lyamin et al. - 2017 - Sleep in the northern fur seal.pdf;/Users/jessiekb/Zotero/storage/IRVRT5D5/Lyamin et al. - 2017 - Sleep in the northern fur seal.pdf;/Users/jessiekb/Zotero/storage/QN7Z8TF8/Lyamin et al. - 2017 - Sleep in the northern fur seal.pdf;/Users/jessiekb/Zotero/storage/XLI7DR7K/Lyamin et al. - 2017 - Sleep in the northern fur seal.pdf;/Users/jessiekb/Zotero/storage/ZFVDWISG/Lyamin et al. - 2017 - Sleep in the northern fur seal.pdf;/Users/jessiekb/Zotero/storage/6W47I6NW/S0959438817300053.html}
}

@article{lyaminSleepOstrichChicks2021,
  title = {Sleep in Ostrich Chicks ( {{{\emph{Struthio}}}}{\emph{ Camelus}} )},
  author = {Lyamin, Oleg I and Kibalnikov, Anton S and Siegel, Jerome M},
  year = {2021},
  month = may,
  journal = {Sleep},
  volume = {44},
  number = {5},
  pages = {zsaa259},
  issn = {0161-8105, 1550-9109},
  doi = {10.1093/sleep/zsaa259},
  urldate = {2021-05-16},
  abstract = {It has been reported that adult ostriches displayed the longest episodes of rapid eye movement (REM) sleep (up to 5 min) and more REM sleep (24\% of the nighttime) than any other bird species. If the mammalian ontogenetic trend exists in the ostrich, then the amounts of REM and the duration of sleep episodes in young ostriches may be greater than those reported in adults. We investigated sleep in 1.5--3.5 month old ostrich chicks. Recordings were conducted during nighttime (20:00--08:00), the main sleep period in ostriches, which are diurnal. The polygrams were scored in 4-s epochs for waking, non-rapid eye movement (NREM) sleep and REM sleep, as in other bird studies. REM sleep in ostrich chicks occurred during both cortical EEG activation and during slow waves, as was described in adult ostriches. The chicks spent 69.3\% {\textpm} 1.5\% of the night in NREM sleep. REM sleep occupied 14.1\% {\textpm} 1.8\% of the night or 16.8\% {\textpm} 2.0\% of nighttime sleep. Episodes of REM sleep lasted on average 10 {\textpm} 1 s and ranged between 4 and 40 s. Therefore, the total amount and duration of REM sleep episodes in ostrich chicks were substantially smaller than reported in adult ostriches while the amounts of NREM sleep did not greatly differ. The developmental profile of REM sleep ontogenesis in the ostrich may be remarkably different from what has been reported in all studied mammals and birds.},
  langid = {english},
  file = {/Users/jessiekb/Zotero/storage/9R2KNDUE/Lyamin et al. - 2021 - Sleep in ostrich chicks ( Struthio camelus .pdf;/Users/jessiekb/Zotero/storage/HI888JHF/Lyamin et al. - 2021 - Sleep in ostrich chicks ( Struthio camelus .pdf;/Users/jessiekb/Zotero/storage/LNUV7X2X/jha et al., 2006.pdf}
}

@article{lyaminSleepWakefulnessSouthern2002,
  title = {Sleep and Wakefulness in the Southern Sea Lion},
  author = {Lyamin, Oleg I. and Mukhametov, Lev M. and Chetyrbok, Igor S. and Vassiliev, Alexander V.},
  year = {2002},
  month = jan,
  journal = {Behavioural Brain Research},
  volume = {128},
  number = {2},
  pages = {129--138},
  issn = {01664328},
  doi = {10.1016/S0166-4328(01)00317-5},
  urldate = {2021-07-26},
  abstract = {We recorded an electroencephalogram from the two hemispheres, a neck musculature electromyogram, an electrooculogram, and respiratory acts during sleep and wakefulness on land in three 1-year-old sea lion females for 3 or 4 consecutive days. On average active wakefulness (AW) occupied 20.4 9 2.0\% of the 24-h period; quiet wakefulness (QW) 54.9 92.5\%; slow wave sleep (SWS) 15.0 92.5\% and paradoxical sleep (PS) 9.7 9 2.0\%. Between 30 and 50\% (average 39.1 93.4\%) of total sleep time was spent in PS. From 8 to 31 episodes of PS were recorded per day (average 17 96 per day), with the longest episode lasting 20 min (average 5.6 90.5 min). Episodes of interhemispheric EEG asymmetry accounted for 5.5 9 1.3\% of total SWS time. Respiratory pauses in these animals varied in QW between 4 and 36 s (average 15.7 9 0.4 s), in SWS between 11 and 37 s (20.9 9 0.6 s) and in PS between 2 and 69 s (15.0 91.5 s). AW, QW, SWS and PS were approximately equally distributed between light (07:00--19:00) and dark time (19:00--07:00). The low amount of SWS with interhemispheric EEG asymmetry, the high proportion of PS in total sleep time and the nearly even distribution of sleep and wakefulness over the 24-h period could be both species-specific features and/or ontogenetic characteristics of the animals studied. {\copyright} 2002 Elsevier Science B.V. All rights reserved.},
  langid = {english},
  keywords = {Interhemispheric EEG asymmetry,Otariidae,Paradoxical sleep,Pinnipeds,Slow wave sleep,Southern sea lion},
  file = {/Users/jessiekb/Zotero/storage/CNZ6UP2Q/rattenborg2008.pdf;/Users/jessiekb/Zotero/storage/GZZWYEXZ/Lyamin et al. - 2002 - Sleep and wakefulness in the southern sea lion.PDF;/Users/jessiekb/Zotero/storage/UJTZWQPK/Lyamin et al. - 2002 - Sleep and wakefulness in the southern sea lion.PDF;/Users/jessiekb/Zotero/storage/ZP3MW532/Lyamin et al. - 2002 - Sleep and wakefulness in the southern sea lion.pdf;/Users/jessiekb/Zotero/storage/LM664CY3/S0166432801003175.html}
}


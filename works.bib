@article{matsumuraUnderwaterSurfaceBehavior2011,
  title = {Underwater and Surface Behavior of Homing Juvenile Northern Elephant Seals},
  author = {Matsumura, Moe and Watanabe, Yuuki Y. and Robinson, Patrick W. and Miller, Patrick J. O. and Costa, Daniel P. and Miyazaki, Nobuyuki},

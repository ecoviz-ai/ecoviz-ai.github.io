@article{bishopRoboticObservationsHigh2016,
  title = {Robotic Observations of High Wintertime Carbon Export in {{California}} Coastal Waters},
  author = {Bishop, James K. B. and Fong, Michael B. and Wood, Todd J.},
  year = {2016},
  month = may,
  journal = {Biogeosciences},
  volume = {13},

  title = {Robotic {{Observations}} of {{Enhanced Carbon Biomass}} and {{Export}} at 55{$^\circ$}{{S During SOFeX}}},
  author = {Bishop, James K. B. and Wood, Todd J. and Davis, Russ E. and Sherman, Jeffrey T.},

@article{keremCerebralToleranceAsphyxial1973,
  title = {Cerebral Tolerance to Asphyxial Hypoxia in the Harbor Seal},
  author = {Kerem, D. and Elsner, R.},
  year = {1973},
  month = nov,
  journal = {Respiration Physiology},
  volume = {19},
  number = {2},
  pages = {188--200},
  issn = {00345687},
  doi = {10.1016/0034-5687(73)90077-7},
  urldate = {2021-06-03},
  abstract = {Young unanesthetized restrained harbor seals. Phoca uitulina were subjected to simulated maximum dives by submerging their heads until the appearance of reversible characteristic hypoxic patterns in their electroencephalogram (EEG endpoint). The mean endpoint PO,, defined as the oxygen tension in the blood at the EEG endpoint, was 10 mm Hg in arterial and 2.5 mm Hg in cerebral venous blood. These values, lower than those reported for terrestrial laborabory mammals, held over a wide range of blood pH and Pco2 values. During the last five minutes of the dive (averaging 18 min in duration) cerebral (a-v)C,, diminished and cerebral (v-a) lactate content increased. Independent measurements of blood flow in the intravertebral vein showed that cerebral blood flow during diving was about two-thirds of the pre-dive value and that it remained substantially unchanged throughout the dive. It is concluded that during prolonged apneic asphyxia the seal's brain, unlike that of terrestrial mammals, tolerates a definite period of go1 reduction prior to the onset of gross functional impairment.},
  langid = {english},
  keywords = {Cerebral blood flow,Electroencephalogram Cerebral metabolism,Hypoxia Diving mammal,Tolerance to asphyxia},
  file = {/Users/jessiekb/Zotero/storage/326DIF5H/Kerem and Elsner - 1973 - Cerebral tolerance to asphyxial hypoxia in the har.pdf;/Users/jessiekb/Zotero/storage/CRFPPH9V/Kerem and Elsner - 1973 - Cerebral tolerance to asphyxial hypoxia in the har.pdf;/Users/jessiekb/Zotero/storage/FGFD9P94/Kerem and Elsner - 1973 - Cerebral tolerance to asphyxial hypoxia in the har.pdf;/Users/jessiekb/Zotero/storage/KC9ZBGSC/Kerem and Elsner - 1973 - Cerebral tolerance to asphyxial hypoxia in the har.pdf;/Users/jessiekb/Zotero/storage/XBDB6MI8/Kerem and Elsner - 1973 - Cerebral tolerance to asphyxial hypoxia in the har.pdf}

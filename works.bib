@article{williamsQualitativeQuantitativeCharacteristics2012,
  title = {Qualitative and {{Quantitative Characteristics}} of the {{Electroencephalogram}} in {{Normal Horses After Sedation}}},
  author = {Williams, D. C. and Aleman, M. and Tharp, B. and Fletcher, D. J. and Kass, P. H. and Steffey, E. P. and Lecouteur, R. A. and Holliday, T. A.},
  year = {2012},
  journal = {Journal of Veterinary Internal Medicine},
  volume = {26},
  number = {3},

@article{naitoUnravellingMysteriesMesopelagic2013,
  title = {Unravelling the Mysteries of a Mesopelagic Diet: {{A}} Large Apex Predator Specializes on Small Prey},
  author = {Naito, Yasuhiko and Costa, Daniel P. and Adachi, Taiki and Robinson, Patrick W. and Fowler, Melinda and Takahashi, Akinori},
  year = {2013},
  journal = {Functional Ecology},
  volume = {27},
  number = {3},
  pages = {710--717},
  issn = {02698463},
  doi = {10.1111/1365-2435.12083},
  abstract = {To gain insight into the foraging behaviour of deep diving seals, we developed a long-term jaw-motion recorder, which successfully measured the feeding attempts of four post-breeding female northern elephant seals for 55-68 days during migration in the north-east Pacific Ocean. Using the jaw-motion recorders in conjunction with satellite tracking data, we first reveal the three-dimensional fine-scale distribution of deep foraging activity in the north-east Pacific Ocean. A large number of jaw-motion events (23 817-58 766 during 2925-4178 dives, per seal) were observed with diel patterns suggesting their dependency on small mesopelagic prey. Calculations using at-sea field metabolic-rate and the photographs concurrently obtained by the head-mounted camera indicated feeding on small mesopelagic prey (10-20 g) including lantern fish (F. Myctophidae). The foraging behaviour of the northern elephant seal contrasts with echolocating toothed whales, which make fewer feeding attempts, suggesting the whales forage more selectively. We hypothesize that the continuous diving mode exhibited by this seal could be attributed to their reliance on small prey and their less efficient 'passive sensors' for prey search, that is, their vision or whiskers to detect prey. {\copyright} 2013 The Authors. Functional Ecology {\copyright} 2013 British Ecological Society.},
  keywords = {Accelerometer,Bio-logging,Deep diving behaviour,Jaw-motion recorder,Mesopelagic foraging,Northern elephant seals,Seal-head camera},
  file = {/Users/jessiekb/Zotero/storage/85AZADJA/Naito et al. - 2013 - Unravelling the mysteries of a mesopelagic diet A.pdf;/Users/jessiekb/Zotero/storage/CKKCQJKH/Naito et al. - 2013 - Unravelling the mysteries of a mesopelagic diet a.pdf}

@article{hsuEmpoweringLocalCommunities2022,
  title = {Empowering Local Communities Using Artificial Intelligence},
  author = {Hsu, Yen-Chia and Huang, Ting-Hao `Kenneth' and Verma, Himanshu and Mauri, Andrea and Nourbakhsh, Illah and Bozzon, Alessandro},
  year = {2022},

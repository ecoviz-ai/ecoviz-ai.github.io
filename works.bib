  langid = {english},
  keywords = {Environmental impact,Natural hazards,notion,Physical oceanography},
  file = {/Users/jessiekb/Zotero/storage/TPY76582/Menéndez et al. - 2020 - The Global Flood Protection Benefits of Mangroves.pdf}
}

@article{miaoIterativeHumanAutomated2021,

@article{kassiriElectronicSleepStage2017,
  title = {Electronic {{Sleep Stage Classifiers}}: {{A Survey}} and {{VLSI Design Methodology}}},
  shorttitle = {Electronic {{Sleep Stage Classifiers}}},
  author = {Kassiri, Hossein and Chemparathy, Aditi and Salam, M. Tariqus and Boyce, Richard and Adamantidis, Antoine and Genov, Roman},

@article{cheesemanCollaborativeNearcomprehensiveNorth2023,
  title = {A Collaborative and Near-Comprehensive {{North Pacific}} Humpback Whale Photo-{{ID}} Dataset},
  author = {Cheeseman, Ted and Southerland, Ken and Acebes, Jo Marie and Audley, Katherina and Barlow, Jay and Bejder, Lars and Birdsall, Caitlin and Bradford, Amanda L. and Byington, Josie K. and Calambokidis, John and Cartwright, Rachel and Cedarleaf, Jen and Chavez, Andrea Jacqueline Garc{\'i}a and Currie, Jens J. and De Weerdt, Jo{\"e}lle and Doe, Nicole and {Doniol-Valcroze}, Thomas and Dracott, Karina and Filatova, Olga and Finn, Rachel and Flynn, Kiirsten and Ford, John K. B. and {Frisch-Jord{\'a}n}, Astrid and Gabriele, Christine M. and Goodwin, Beth and Hayslip, Craig and Hildering, Jackie and Hill, Marie C. and Jacobsen, Jeff K. and {Jim{\'e}nez-L{\'o}pez}, M. Esther and Jones, Meagan and Kobayashi, Nozomi and Lyman, Edward and Malleson, Mark and Mamaev, Evgeny and Mart{\'i}nez Loustalot, Pamela and Masterman, Annie and Matkin, Craig and McMillan, Christie J. and Moore, Jeff E. and Moran, John R. and Neilson, Janet L. and Newell, Hayley and Okabe, Haruna and Olio, Marilia and Pack, Adam A. and Palacios, Daniel M. and Pearson, Heidi C. and {Quintana-Rizzo}, Ester and Ram{\'i}rez Barrag{\'a}n, Raul Fernando and Ransome, Nicola and {Rosales-Nanduca}, Hiram and Sharpe, Fred and Shaw, Tasli and Stack, Stephanie H. and Staniland, Iain and Straley, Jan and Szabo, Andrew and Teerlink, Suzie and Titova, Olga and Urban R., Jorge and {van Aswegen}, Martin and {de Morais}, Marcel Vinicius and {von Ziegesar}, Olga and Witteveen, Briana and Wray, Janie and Yano, Kymberly M. and Zwiefelhofer, Denny and Clapham, Phil},
  year = {2023},
  month = jun,
  journal = {Scientific Reports},
  volume = {13},
  number = {1},
  pages = {10237},
  publisher = {Nature Publishing Group},
  issn = {2045-2322},
  doi = {10.1038/s41598-023-36928-1},
  urldate = {2024-05-12},
  abstract = {We present an ocean-basin-scale dataset that includes tail fluke photographic identification (photo-ID) and encounter data for most living individual humpback whales (Megaptera novaeangliae) in the North Pacific Ocean. The dataset was built through a broad collaboration combining 39 separate curated photo-ID catalogs, supplemented with community science data. Data from throughout the North Pacific were aggregated into 13 regions, including six breeding regions, six feeding regions, and one migratory corridor. All images were compared with minimal pre-processing using a recently developed image recognition algorithm based on machine learning through artificial intelligence; this system is capable of rapidly detecting matches between individuals with an estimated 97--99\% accuracy. For the 2001--2021 study period, a total of 27,956 unique individuals were documented in 157,350 encounters. Each individual was encountered, on average, in 5.6 sampling periods (i.e., breeding and feeding seasons), with an annual average of 87\% of whales encountered in more than one season. The combined dataset and image recognition tool represents a living and accessible resource for collaborative, basin-wide studies of a keystone marine mammal in a time of rapid ecological change.},
  copyright = {2023 The Author(s)},
  langid = {english},
  keywords = {Conservation biology,Marine biology,notion,Population dynamics,Software},

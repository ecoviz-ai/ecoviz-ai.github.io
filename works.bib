@article{macelwainMisunderstoodMorays1977,
  title = {The Misunderstood Morays},
  author = {MacElwain, W},
  year = {1977},
  journal = {Sea Front.},
  volume = {23},
  number = {2},
  pages = {96--100},
  abstract = {Most morays (Muraenidae) live in warm tropical seas although a few spp are found in more temperate waters. They shelter during the day in holes or crevices and feed during the night. They have a keen sense of smell with which to locate food. They feed on may types of fish and cephalopods. Some of which have evolved protection by emitting odours, such as spp of parrot fish, or by emitting ink which anesthetizes the moray's sense of smell for several hrs. The sharp teeth of morays are adapted for grasping or swallowing. There are numerous spp of shrimps and fishes that form symbiotic relationships with morays, picking parasites and often cleaning the insides of the eels' mouths. Morays are potentially dangerous because of their teeth and size, however the danger only occurs when they are provoked, not if they are left alone.},
  keywords = {ASFA 1: Biological Sciences & Living Resources,Avoidance reactions,Behaviour,Chemoreception,Feeding,Food organisms,Marine,Marine ecology,Muraenidae,Q1 08341:General,Symbionts}
}

